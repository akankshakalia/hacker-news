{
  "name": "hacker-news",
  "version": "1.0.0",
  "description": "Hacker News React App",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --host 0.0.0.0 --inline --content-base .",
    "start:dev": "webpack-dev-server --mode development --open --hot",
    "build": "webpack --mode production",
    "heroku-postbuild": "webpack -p",
    "eslint": "eslint --fix",
    "eslintf ": "eslint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://git.heroku.com/mat-hacker-news.git"
  },
  "keywords": [
    "news",
    "hacker"
  ],
  "author": "Mathew Pauly",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.9.4",
    "axios": "^0.19.2",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.4.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "compression-webpack-plugin": "^3.1.0",
    "css-loader": "^3.5.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.4.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.0",
    "prop-types": "^15.7.2",
    "react-google-charts": "^3.0.15",
    "react-test-renderer": "^16.13.1",
    "regenerator-runtime": "^0.13.5",
    "sass-loader": "^7.1.0",
    "style-loader": "^1.2.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-md5-hash": "0.0.6"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2"
  },
  "sideEffects": [
    "*.css"
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss|sass|gif)$": "identity-obj-proxy"
    },
    "transform": {
      ".*": "./node_modules/babel-jest"
    },
    "testRegex": "(\\.|/)(test)\\.js?$",
    "roots": [
      "src"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ]
  }
}
